digraph {
	node [fontsize=9 height=0.2 shape=circle width=0.2]
	-1 [fillcolor=lightgray shape=box style=filled]
	-2 [fillcolor=lightgray shape=box style=filled]
	0 [fillcolor=lightblue style=filled]
	1 [fillcolor=lightblue style=filled]
	2 [fillcolor=white style=filled]
	3 [fillcolor=white style=filled]
	-2 -> 1 [color=green penwidth=1 style=solid]
	-1 -> 0 [color=green penwidth=1 style=solid]
	-2 -> 0 [color=green penwidth=1 style=solid]
	-1 -> 2 [color=green penwidth=1 style=solid]
	2 -> 3 [color=green penwidth=1 style=solid]
	3 -> 1 [color=green penwidth=1 style=solid]
	3 -> 0 [color=green penwidth=1 style=solid]
}
